{
  "name": "ny-api",
  "version": "1.6.0",
  "description": "API REST using NodeJS and KOA framework, typescript. TypeORM for SQL with class-validators. Middlewares JWT, CORS, Winston Logger. Swagger API docs",
  "main": "dist/server.js",
  "scripts": {
    "watch-server": "nodemon --watch src -e ts,tsx --exec ts-node src/server.ts",
    "build-ts": "tsc",
    "tslint": "tslint --fix -c tslint.json -p tsconfig.json",
    "serve": "node dist/server.js",
    "build": "npm run tslint && npm run build-ts",
    "start": "npm run serve"
  },
  "engines": {
    "node": "10.x.x",
    "npm": "6.x.x"
  },
  "author": "Jariel Balberona",
  "license": "MIT",
  "homepage": "https://github.com/jawwwr/ny-api#readme",
  "keywords": [
    "typescript",
    "koa",
    "koa2",
    "jwt",
    "winston",
    "boilerplate",
    "typeORM",
    "class-validator",
    "helmet",
    "sql",
    "api rest",
    "heroku",
    "cors",
    "github actions"
  ],
  "repository": "github:jawwwr/ny-api",
  "devDependencies": {
    "@types/cron": "^1.7.1",
    "@types/dotenv": "^6.1.1",
    "@types/koa": "2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa__cors": "^2.2.3",
    "@types/mysql": "^2.15.8",
    "@types/node": "^12.0.12",
    "@types/shelljs": "^0.8.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@types/socket.io": "^2.1.4",
    "axios": "^0.21.1",
    "class-validator": "^0.9.1",
    "cron": "^1.7.2",
    "dotenv": "^8.0.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.2.0",
    "koa-jwt": "^3.3.2",
    "koa-router": "^7.4.0",
    "koa-swagger-decorator": "^1.5.16",
    "mysql": "^2.17.1",
    "pg": "^7.4.3",
    "pg-connection-string": "^2.0.0",
    "reflect-metadata": "^0.1.12",
    "socket.io": "^2.3.0",
    "splitwise": "^1.0.0",
    "splitwise-node": "^0.3.3",
    "typeorm": "^0.2.18",
    "winston": "^3.2.1"
  }
}
